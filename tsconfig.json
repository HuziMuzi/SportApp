{
  "extends": "@react-native/typescript-config/tsconfig.json",
//  "include": ["src/**/*"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "@screens/*": ["src/screens/*"],
      "types/*": ["src/shared/types/*"]
    },
    "noImplicitAny": false,
    /* Completeness */
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
